{
  "family": "aws-architect-agent",
  "containerDefinitions": [
    {
      "name": "aws-architect-agent",
      "image": "269550163595.dkr.ecr.ap-northeast-2.amazonaws.com/aws-architect-agent:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "8080",
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aws-architect-agent",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "systemControls": []
    }
  ],
  "taskRoleArn": "arn:aws:iam::269550163595:role/ecsAdminRole",
  "executionRoleArn": "arn:aws:iam::269550163595:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}